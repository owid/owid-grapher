{
    "$schema": "./node_modules/oxfmt/configuration_schema.json",
    "trailingComma": "es5",
    "semi": false,
    "printWidth": 80,
    "sortPackageJson": false,
    "ignorePatterns": [
        ".tscache",
        "/bakedSite",
        "/localBake",
        "datasetsExport",
        "itsJustJavascript",
        "package.json",
        "coverage",
        "packages/@ourworldindata/*/dist/",
        "dist/",
        "grapherData/",
        ".vscode/",
        ".claude/settings.local.json",
        "*.handlebars"
    ]
}
