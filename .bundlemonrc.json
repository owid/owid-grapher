{
    "baseDir": "./dist/assets",
    "defaultCompression": "brotli",
    "files": [
        {
            "path": "owid.mjs",
            "maxSize": "700kB",
            "maxPercentIncrease": 5
        },
        {
            "path": "owid.css",
            "maxSize": "55kB"
        }
    ],

    "reportOutput": [
        [
            "github",
            {
                "checkRun": true,
                "commitStatus": true,
                "prComment": "on-failure"
            }
        ]
    ]
}
