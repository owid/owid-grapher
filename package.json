        "startViteServer": "vite dev",
        "startSiteFront": "./devTools/vite/startVite.sh",
        "startLocalBakeServer": "http-server ./localBake -p 3000",
        "testBundlemon": "yarn buildViteSite && bundlemon",
        "testLint": "eslint .",
        "testLintChanged": "bash -c 'files=$((git diff --name-only --diff-filter=ACMRTUXB && git diff --name-only --cached --diff-filter=ACMRTUXB) | sort -u | grep -E \"\\.(tsx?|jsx?|ts|js)$\" | tr \"\\n\" \" \"); if [ -n \"$files\" ]; then eslint $files; else echo \"No uncommitted files to lint\"; fi'",
